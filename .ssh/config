Include ~/.ssh/local_config

Host *
  AddKeysToAgent yes
  ForwardAgent yes
  Compression yes
  TCPKeepAlive yes

  # allow multiple sessions to share a single TCP socket
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h:%p
  ControlPersist 5m

Match host * !exec "[ -e ~/.ssh/sockets/master-%r@%h:%p ]"
  RemoteForward 52698 127.0.0.1:52698
