*.geometry: 120x32

*.font:         xft:Hack Nerd Font:pixelsize=12
*.letterSpace:  -1
*.scrollBar:    false

Xft.dpi: 80
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!Xft.dpi:        100
!Xft.antialias:  true
!Xft.hinting:    true
!Xft.autohint:   true
!Xft.hintstyle:  hintfull

! special
*.foreground:   #f6f6ef
*.background:   #1c1c1c
*.cursorColor:  #f6f6ef

! black
*.color0:       #1d1d19
*.color8:       #5f5f5f

! red
*.color1:       #d11c24
*.color9:       #ed0053

! green
*.color2:       #a7d42c
*.color10:      #a7d42c

! yellow
*.color3:       #d8d067
*.color11:      #d8d067

! blue
*.color4:       #61b9d0
*.color12:      #61b9d0

! magenta
*.color5:       #695abc
*.color13:      #695abc

! cyan
*.color6:       #d63865
*.color14:      #d63865

! white
*.color7:       #ffffff
*.color15:      #ffffff
